{
  "name": "tourze/doctrine-logger-bundle",
  "description": "Doctrine Logger Bundle",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "ext-mbstring": "*",
    "doctrine/doctrine-bundle": "^2.13",
    "doctrine/orm": "^2.20 || ^3.0",
    "symfony/config": "^6.4",
    "symfony/dependency-injection": "^6.4",
    "symfony/http-kernel": "^6.4",
    "symfony/framework-bundle": "^6.4",
    "symfony/yaml": "^6.4 || ^7.1",
    "symfony/stopwatch": "^6.4",
    "symfony/doctrine-bridge": "^6.4",
    "symfony/service-contracts": "^3.5",
    "tourze/backtrace-helper": "~0.0.5",
    "psr/log": "^3|^2|^1",
    "yiisoft/strings": "^2.1"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\DoctrineLoggerBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\DoctrineLoggerBundle\\Tests\\": "tests"
    }
  },
  "scripts": {
    "phpstan": "phpstan analyse src -l 1"
  }
}